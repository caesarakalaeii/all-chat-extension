{
  "name": "allchat-extension",
  "version": "1.2.6",
  "private": true,
  "description": "Browser extension to replace native platform chat with All-Chat",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
